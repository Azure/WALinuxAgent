{
  "image-set": "endorsed-daily",
  "image-sets": {
    "endorsed-daily": [
      "centos_78_verbose",
      "suse_12_verbose",
      "rhel_78_verbose",
      "ubuntu_1604_verbose",
      "ubuntu_1804_verbose"
    ]
  },
  "packer-provisioner-extensions": {
    "prepend": [
      {
        "execute_command": "chmod +x {{ .Path }}; {{ .Vars }} sudo -E {{ .Path }}",
        "inline": [
          "cd /tmp/WALinuxAgent",
          "sed -i 's/_EXTENSION_EVENT_COLLECTION_PERIOD = datetime.timedelta(minutes=5)/_EXTENSION_EVENT_COLLECTION_PERIOD = datetime.timedelta(seconds=30)/g' azurelinuxagent/ga/collect_telemetry_events.py"
        ],
        "inline_shebang": "/bin/sh -ex",
        "type": "shell",
        "timeout": "1m"
      }
    ]
  },
  "environment": {
    "test_regions": [
      "South Central US"
    ],
    "servicePrincipalName": {
      "subscription_id": "8e037ad4-618f-4466-8bc8-5099d41ac15b",
      "client_id": "cd8bc24d-406a-479d-b3e4-855d852e8a9d",
      "tenant_id": "72f988bf-86f1-41af-91ab-2d7cd011db47",
      "object_id": "85f9bd52-5e31-41b8-9141-9b30d5815d66"
    }
  }
}